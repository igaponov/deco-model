{
  "name": "deco-model",
  "version": "0.0.9",
  "description": "Decompression models",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn run clean:dist && yarn run bundle:dist && yarn run copy:dist",
    "clean:dist": "rimraf dist && mkdir dist",
    "bundle:dist": "babel ./src -d ./dist --ignore \"src/**/*.spec.js\",\"src/**/*.test.js\"",
    "copy:dist": "cp index.js.flow dist",
    "prepublishOnly": "yarn test && yarn build",
    "test": "node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/igaponov/deco-model.git"
  },
  "keywords": [
    "decompression",
    "model",
    "diving"
  ],
  "author": "Igor Gaponov",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/igaponov/deco-model/issues"
  },
  "homepage": "https://github.com/igaponov/deco-model#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0"
  }
}
